{
  "name": "ricardofiorani/video-oembed",
  "description": "A Simple and efficient PHP Video URL Parser that gives you thumbnails and embed codes for various services such as Youtube, Vimeo, DailyMotion and Facebook",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Fiorani",
      "email": "ricardo.fiorani@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/http-message": "^1.0",
    "php-http/discovery": "^1.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^8.4",
    "ricardofiorani/guzzle-psr18-adapter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "RicardoFiorani\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RicardoFioraniTests\\": "tests/"
    }
  }
}
